{
    "builds": [{
        "src": "configurations/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "150mb", "runtime":"python3.7" }
    },

    {
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"staticfiles_build"
        }

    }

],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "configurations/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
    
}