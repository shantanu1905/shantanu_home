{
    "builds": [{
        "src": "configurations/wsgi.py",
        "use": "@ardnt/configurations",
        "config": { "maxLambdaSize": "15mb" , "runtime": "python3.9" , "wsgiApplicationName": "application"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel_app/wsgi.py"
        }
    ]
}